{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "vsco backup-settings",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/app/vsco.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "args": [
                "backup-settings",
                "./tmp"
            ]
        }
    ]
}
